ry-list-box\">\r\n      <el-table\r\n          :data=\"\r\n          tvs.slice((currentPage - 1) * pagesize, currentPage * pagesize)\"\r\n          style=\"width: 100%\"\r\n          class=\"table_color\"\r\n          :row-class-name=\"tableRowClassName\"\r\n          :header-cell-style=\"getRowClass\">\r\n        <el-table-column width=\"20\"></el-table-column>\r\n        <el-table-column prop=\"id\" label=\"id\" width=\"220\">\r\n          <template slot-scope=\"scope\">\r\n            <span\r\n                v-text=\"scope.row.id\"\r\n                style=\"color: white; font-weight: bold\">\r\n            </span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"name\" label=\"接口名称\" width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n             <span\r\n                 v-text=\"scope.row.name\"\r\n                 style=\"color: white; font-weight: bold\">\r\n             </span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"url\" label=\"接口地址\" width=\"400\">\r\n          <template slot-scope=\"scope\">\r\n            <span\r\n                v-text=\"scope.row.url\"\r\n                style=\"\r\n                width: 520px;\r\n                overflow: hidden;\r\n                white-space: nowrap;\r\n                text-overflow: ellipsis;\r\n                color: white;\r\n                font-weight: bold;\">\r\n            </span>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column label=\"头像\" width=\"190\">\r\n          <template slot-scope=\"scope\">\r\n            <div>\r\n\r\n              <el-image\r\n                  style=\"width: 100px; height: 40px\"\r\n                  :src=\"scope.row.icon\"\r\n                  fit=\"contain\"></el-image>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column prop=\"user\" label=\"添加人\" width=\"148\">\r\n          <template slot-scope=\"scope\">\r\n           <span\r\n               v-text=\"scope.row.user\"\r\n               style=\"color: white; font-weight: bold\">\r\n             </span>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column width=\"200\"></el-table-column>\r\n      </el-table>\r\n\r\n      <div class=\"lables-dialog-box\">\r\n        <el-dialog :title=\"tvText\" :visible.sync=\"editTv\" width=\"600px\">\r\n          <div class=\"lable-add_box\">\r\n            <el-form label-width=\"80px\">\r\n              <el-form-item label=\"接口名称\">\r\n                <el-input v-model=\"tvItems.name\" @input=\"change($event)\" ></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"接口地址\">\r\n                <el-input v-model=\"tvItems.url\" @input=\"change($event)\" ></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"图标\">\r\n                <el-input\r\n                    v-model=\"tvItems.icon\"\r\n                    @input=\"change($event)\"\r\n                ></el-input>\r\n              </el-form-item>\r\n            </el-form>\r\n          </div>\r\n\r\n          <span slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"cancelAddItem\" size=\"mini\" type=\"danger\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"addItem\" size=\"mini\">确 定</el-button>\r\n          </span>\r\n        </el-dialog>\r\n      </div>\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"currentPage\"\r\n            :page-sizes=\"[8, 9, 10]\"\r\n            :page-size=\"pagesize\"\r\n            layout=\"total, sizes,prev, pager, next\"\r\n            :total=\"tvs.length\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      show: false,\r\n      currentPage: 1, //默认显示页面为1\r\n      pagesize: 8, //    每页的数据条数\r\n      tvs: [], //需要data定义一些，tableData定义一个空数组，请求的数据都是存放这里面\r\n      editTv: false,\r\n      tvText: \"\",\r\n      username:null,\r\n      tvId: null,\r\n      tvItems: [\r\n        {\r\n          name: \"\",\r\n          url: \"\",\r\n          user:\"\",\r\n          icon:\"\"\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    //控制行颜色\r\n    tableRowClassName({ row, rowIndex }) {\r\n      if ((rowIndex + 1) % 2 === 0) {\r\n        return \"double\"; //对应的类\r\n      } else {\r\n        return \"single\"; //对应的类\r\n      }\r\n    },\r\n    //控制表格头部颜色\r\n    getRowClass({ rowIndex }) {\r\n      if (rowIndex == 0) {\r\n        return \"background:#2d3a5c\";\r\n      } else {\r\n        return \"\";\r\n      }\r\n    },\r\n\r\n    edit: function (item) {\r\n      this.tvItems.name = item.name;\r\n      this.tvItems.url = item.url;\r\n      this.tvItems.user = item.user;\r\n      this.tvId = item.id;\r\n      this.tvText = \"修改电影接口\";\r\n      this.editTv = true;\r\n    },\r\n\r\n    addItem: function () {\r\n      console.log(\"执行的添加方法\");\r\n      this.$axios\r\n          .post(\"/admin/tv-add\", {\r\n            name: this.tvItems.name,\r\n            url:  this.tvItems.url,\r\n            user: this.username,\r\n            icon: this.tvItems.icon\r\n          })\r\n          .then((result) => {\r\n            if (result.data.code === 20000) {\r\n              this.sendMessage(\"添加成功\", \"success\");\r\n              this.getData();\r\n            } else {\r\n              this.sendMessage(\"添加失败\", \"error\");\r\n            }\r\n          }),\r\n          (response) => {\r\n            this.sendMessage(\"请求失败\", \"error\");\r\n          };\r\n      this.editTv = false;\r\n    },\r\n\r\n    addItemTv: function () {\r\n      this.editTv = true;\r\n    },\r\n\r\n    cancelAddItem: function () {\r\n      this.editTv = false;\r\n    },\r\n\r\n    sendMessage: function (msg, msgType) {\r\n      this.$message({\r\n        message: msg,\r\n        center: true,\r\n        type: msgType,\r\n      });\r\n    },\r\n\r\n    getData() {\r\n      this.$axios.get(\"/user/check-token\").then((response) => {\r\n        let res = response.data;\r\n        if (res.code === 20000) {\r\n          console.log(\"username is ===>\"+res.data.name)\r\n          this.username = res.data.name\r\n        }\r\n      }), (response) => {console.log(\"error\");};\r\n\r\n      this.show = true;\r\n      this.$axios.get(\"/user/tv-list\").then(\r\n          (response) => {\r\n            console.log(response.data.data);\r\n            console.log(response.data.data.content);\r\n            this.tvs = response.data.data;\r\n          },\r\n          (response) => {\r\n            console.log(\"error\");\r\n          }\r\n      );\r\n    },\r\n    //每页下拉显示数据\r\n    handleSizeChange: function (size) {\r\n      this.pagesize = size;\r\n      /*console.log(this.pagesize) */\r\n    },\r\n    //点击第几页\r\n    handleCurrentChange: function (currentPage) {\r\n      this.currentPage = currentPage;\r\n      /*console.log(this.currentPage) */\r\n    },\r\n\r\n    change(e) {\r\n      this.$forceUpdate();\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getData();\r\n    window.addEventListener(\"keydown\", this.keyDown);\r\n  },\r\n  destroyed() {\r\n    window.removeEventListener(\"keydown\", this.keyDown, false);\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.table {\r\n  width: 900px;\r\n  margin: 100px auto;\r\n}\r\n/*更改表格颜色*/\r\n.double {\r\n  background: #262d47 !important;\r\n}\r\n.single {\r\n  background: #2a314d !important;\r\n}\r\n\r\n.category-list-box {\r\n  margin-top: 20px;\r\n  margin-right: 4px;\r\n}\r\n\r\n.el-table__body tr:hover > td {\r\n  background-color: #134087 !important;\r\n}\r\n\r\n.el-table__body-wrapper::-webkit-scrollbar {\r\n  /*width: 0;宽度为0隐藏*/\r\n  width: 0px;\r\n}\r\n\r\nbody {\r\n  overflow-y: auto;\r\n}\r\n</style>"]}]}