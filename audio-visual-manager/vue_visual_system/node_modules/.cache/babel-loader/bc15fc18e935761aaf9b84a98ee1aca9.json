A,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,OAAA;AACA,OALA,EAMA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,OARA;AAUA,KAlLA;AAmLA;AACA,IAAA,gBAAA,EAAA,0BAAA,IAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA;AACA,KAvLA;AAwLA;AACA,IAAA,mBAAA,EAAA,6BAAA,WAAA,EAAA;AACA,WAAA,WAAA,GAAA,WAAA;AACA;AACA,KA5LA;AA8LA,IAAA,MA9LA,kBA8LA,CA9LA,EA8LA;AACA,WAAA,YAAA;AACA;AAhMA,GApBA;AAsNA,EAAA,OAtNA,qBAsNA;AACA,SAAA,OAAA;AACA,IAAA,MAAA,CAAA,gBAAA,CAAA,SAAA,EAAA,KAAA,OAAA;AACA,GAzNA;AA0NA,EAAA,SA1NA,uBA0NA;AACA,IAAA,MAAA,CAAA,mBAAA,CAAA,SAAA,EAAA,KAAA,OAAA,EAAA,KAAA;AACA;AA5NA,CAAA","sourcesContent":["<template>\r\n  <div v-if=\"show\" class=\"list-box\">\r\n    <div class=\"category-action-bar\">\r\n      <el-button type=\"info\" size=\"mini\" @click=\"addItemuser\"\r\n        >添加用户</el-button\r\n      >\r\n      <!-- 测试提交 -->\r\n    </div>\r\n    <div class=\"category-list-box\">\r\n      <el-table\r\n        :data=\"\r\n          users.slice((currentPage - 1) * pagesize, currentPage * pagesize)\r\n        \"\r\n        style=\"width: 100%\"\r\n        class=\"table_color\"\r\n        :row-class-name=\"tableRowClassName\"\r\n        :header-cell-style=\"getRowClass\"\r\n      >\r\n        <el-table-column width=\"20\"></el-table-column>\r\n        <el-table-column prop=\"id\" label=\"id\" width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n            <span\r\n              v-text=\"scope.row.id\"\r\n              style=\"color: white; font-weight: bold\"\r\n            ></span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"userName\" label=\"用户名\" width=\"146\">\r\n          <template slot-scope=\"scope\">\r\n            <span\r\n              v-text=\"scope.row.userName\"\r\n              style=\"\r\n                width: 240px;\r\n                overflow: hidden;\r\n                white-space: nowrap;\r\n                text-overflow: ellipsis;\r\n                color: white;\r\n                font-weight: bold;\r\n              \"\r\n            ></span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"email\" label=\"邮箱\" width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n            <span\r\n              v-text=\"scope.row.email\"\r\n              style=\"\r\n                width: 200px;\r\n                overflow: hidden;\r\n                white-space: nowrap;\r\n                text-overflow: ellipsis;\r\n                color: white;\r\n                font-weight: bold;\r\n              \"\r\n            ></span>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column label=\"状态\" width=\"100\">\r\n          <template slot-scope=\"scope\">\r\n            <div v-if=\"scope.row.state === '0'\">\r\n              <el-tag type=\"danger\" size=\"medium\">禁用</el-tag>\r\n            </div>\r\n            <div v-if=\"scope.row.state === '1'\">\r\n              <el-tag type=\"success\" size=\"medium\">正常</el-tag>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"创建时间\" width=\"200\">\r\n          <template slot-scope=\"scope\">\r\n            <span\r\n              v-text=\"formatDate(scope.row.createTime)\"\r\n              style=\"color: white; font-weight: bold\"\r\n            ></span>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column prop=\"updateTime\" label=\"修改时间\" width=\"200\">\r\n          <template slot-scope=\"scope\">\r\n            <span\r\n              v-text=\"formatDate(scope.row.updateTime)\"\r\n              style=\"color: white; font-weight: bold\"\r\n            ></span>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column label=\"操作\" width=\"200\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"warning\" size=\"mini\" @click=\"forbidden(scope.row)\">\r\n              禁用\r\n            </el-button>\r\n\r\n            <el-button type=\"danger\" size=\"mini\" @click=\"deleteItem(scope.row)\">\r\n              删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column width=\"90\"></el-table-column>\r\n      </el-table>\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[8, 9, 10]\"\r\n          layout=\"total, sizes,prev, pager, next\"\r\n          :total=\"users.length\"\r\n          background=\"#171c30\"\r\n        >\r\n        </el-pagination>\r\n\r\n        <div class=\"lables-dialog-box\">\r\n          <el-dialog\r\n            title=\"删除提示\"\r\n            :visible.sync=\"deleteDialogShow\"\r\n            width=\"400px\"\r\n          >\r\n            <span\r\n              >你确定要删除<strong\r\n                ><font color=\"red\">{{ deleleteMessage }}</font> </strong\r\n              >这个用户</span\r\n            >\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n              <el-button @click=\"cancelItem\" size=\"mini\" type=\"danger\"\r\n                >取 消</el-button\r\n              >\r\n              <el-button type=\"primary\" @click=\"doDeleteItem\" size=\"mini\"\r\n                >确 定</el-button\r\n              >\r\n            </span>\r\n          </el-dialog>\r\n\r\n          <el-dialog title=\"添加用户\" :visible.sync=\"editUser\" width=\"600px\">\r\n            <div class=\"lable-add_box\">\r\n              <el-form label-width=\"60px\">\r\n                <el-form-item label=\"用户名\">\r\n                  <el-input\r\n                    v-model=\"UsersItems.userName\"\r\n                    @input=\"change($event)\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"密码\">\r\n                  <el-input\r\n                    v-model=\"UsersItems.password\"\r\n                    @input=\"change($event)\"\r\n                    type=\"password\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"邮箱\">\r\n                  <el-input\r\n                    v-model=\"UsersItems.email\"\r\n                    @input=\"change($event)\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </el-form>\r\n            </div>\r\n\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n              <el-button @click=\"cancelAddItem\" size=\"mini\" type=\"danger\"\r\n                >取 消</el-button\r\n              >\r\n              <el-button type=\"primary\" @click=\"addItem\" size=\"mini\"\r\n                >确 定</el-button\r\n              >\r\n            </span>\r\n          </el-dialog>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as dataUtils from \"../../utils/date\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      show: false,\r\n      currentPage: 1, //默认显示页面为1\r\n      pagesize: 8, //    每页的数据条数\r\n      users: [], //需要data定义一些，tableData定义一个空数组，请求的数据都是存放这里面\r\n      deleleteMessage: \"\",\r\n      editUser: false,\r\n      deleteTargeId: \"\",\r\n      deleteDialogShow: false,\r\n      UsersItems: [\r\n        {\r\n          userName: \"\",\r\n          password: \"\",\r\n          email: \"\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    //控制行颜色\r\n    tableRowClassName({ row, rowIndex }) {\r\n      if ((rowIndex + 1) % 2 === 0) {\r\n        return \"double\"; //对应的类\r\n      } else {\r\n        return \"single\"; //对应的类\r\n      }\r\n    },\r\n    //控制表格头部颜色\r\n    getRowClass({ rowIndex }) {\r\n      if (rowIndex == 0) {\r\n        return \"background:#2d3a5c\";\r\n      } else {\r\n        return \"\";\r\n      }\r\n    },\r\n\r\n    keyDown(e) {\r\n      //如果是回车则执行登录方法\r\n      if (e.keyCode == 13) {\r\n        this.addItem();\r\n      }\r\n    },\r\n\r\n    deleteItem: function (item) {\r\n      this.deleteDialogShow = true;\r\n      this.deleleteMessage = item.userName;\r\n      this.deleteTargeId = item.id;\r\n      console.log(item);\r\n    },\r\n\r\n    addItemuser: function () {\r\n      this.editUser = true;\r\n    },\r\n\r\n    addItem: function () {\r\n      this.$axios\r\n        .post(\"/user/add\", {\r\n          userName: this.UsersItems.userName,\r\n          password: this.UsersItems.password,\r\n          email: this.UsersItems.email,\r\n        })\r\n        .then(\r\n          (response) => {\r\n            console.log(response.data.code);\r\n\r\n            if (response.data.code === 20000) {\r\n              this.getData();\r\n              this.$message({\r\n                message: response.data.message,\r\n                center: true,\r\n                type: \"success\",\r\n              });\r\n            } else {\r\n              this.$message({\r\n                message: response.data.message,\r\n                center: true,\r\n                type: \"error\",\r\n              });\r\n              this.getData();\r\n            }\r\n          },\r\n          (response) => {}\r\n        );\r\n      this.UsersItems = {};\r\n      this.editUser = false;\r\n    },\r\n\r\n    cancelAddItem: function () {\r\n      this.editUser = false;\r\n    },\r\n\r\n    cancelItem: function () {\r\n      this.deleteDialogShow = false;\r\n    },\r\n\r\n    forbidden: function (item) {\r\n      this.$axios.get(\"/user/check-token\").then((response) => {\r\n        let res = response.data;\r\n        if (res.code === 20000) {\r\n          if (res.data.roles === \"role_admin\") {\r\n            console.log(\"是超级管理员\");\r\n          } else {\r\n            this.$message({\r\n              message: \"无权访问\",\r\n              center: true,\r\n              type: \"error\",\r\n            });\r\n            console.log(\"是普通管理员\");\r\n          }\r\n        }\r\n      }),\r\n        (response) => {\r\n          console.log(\"error\");\r\n        };\r\n\r\n      this.$axios.get(\"/user/forbidden/\" + item.id).then(\r\n        (response) => {\r\n          console.log(response.data.code);\r\n\r\n          if (response.data.code === 20000) {\r\n            this.getData();\r\n            this.$message({\r\n              message: response.data.message,\r\n              center: true,\r\n              type: \"success\",\r\n            });\r\n          } else {\r\n            this.$message({\r\n              message: response.data.message,\r\n              center: true,\r\n              type: \"error\",\r\n            });\r\n            this.getData();\r\n          }\r\n        },\r\n        (response) => {}\r\n      );\r\n    },\r\n\r\n    doDeleteItem: function () {\r\n      this.$axios.get(\"/user/check-token\").then((response) => {\r\n        let res = response.data;\r\n        if (res.code === 20000) {\r\n          if (res.data.roles === \"role_admin\") {\r\n            console.log(\"是超级管理员\");\r\n            this.$axios.delete(\"/user/\" + this.deleteTargeId).then(\r\n              (response) => {\r\n                if (response.data.code === 20000) {\r\n                  this.getData();\r\n                  this.$message({\r\n                    message: response.data.message,\r\n                    center: true,\r\n                    type: \"success\",\r\n                  });\r\n                } else {\r\n                  this.$message({\r\n                    message: response.data.message,\r\n                    center: true,\r\n                    type: \"error\",\r\n                  });\r\n                  this.getData();\r\n                }\r\n              },\r\n              (response) => {}\r\n            );\r\n          } else {\r\n            this.$message({\r\n              message: \"无权访问\",\r\n              center: true,\r\n              type: \"error\",\r\n            });\r\n            console.log(\"是普通管理员\");\r\n          }\r\n        }\r\n      }),\r\n        this.getData();\r\n      this.deleteDialogShow = false;\r\n    },\r\n\r\n    formatDate: function (dataStr) {\r\n      let data = new Date(dataStr);\r\n      return dataUtils.formatDate(data, \"yyyy年MM月dd日 hh:mm:ss\");\r\n    },\r\n\r\n    getData() {\r\n      this.show = true;\r\n      this.$axios.get(\"/user/list/0/0\").then(\r\n        (response) => {\r\n          // console.log(response.data);\r\n          console.log(response.data.data.content);\r\n          this.users = response.data.data.content;\r\n        },\r\n        (response) => {\r\n          console.log(\"error\");\r\n        }\r\n      );\r\n    },\r\n    //每页下拉显示数据\r\n    handleSizeChange: function (size) {\r\n      this.pagesize = size;\r\n      /*console.log(this.pagesize) */\r\n    },\r\n    //点击第几页\r\n    handleCurrentChange: function (currentPage) {\r\n      this.currentPage = currentPage;\r\n      /*console.log(this.currentPage) */\r\n    },\r\n\r\n    change(e) {\r\n      this.$forceUpdate();\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getData();\r\n    window.addEventListener(\"keydown\", this.keyDown);\r\n  },\r\n  destroyed() {\r\n    window.removeEventListener(\"keydown\", this.keyDown, false);\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.table {\r\n  width: 900px;\r\n  margin: 100px auto;\r\n}\r\n/*更改表格颜色*/\r\n.double {\r\n  background: #262d47 !important;\r\n}\r\n.single {\r\n  background: #2a314d !important;\r\n}\r\n\r\n.category-list-box {\r\n  margin-top: 20px;\r\n  margin-right: 4px;\r\n}\r\n\r\n.el-table__body tr:hover > td {\r\n  background-color: #134087 !important;\r\n}\r\n\r\n.el-table__body-wrapper::-webkit-scrollbar {\r\n  /*width: 0;宽度为0隐藏*/\r\n  width: 0px;\r\n}\r\n\r\nbody {\r\n  overflow-y: auto;\r\n}\r\n</style>"],"sourceRoot":"src/page/user"}]}