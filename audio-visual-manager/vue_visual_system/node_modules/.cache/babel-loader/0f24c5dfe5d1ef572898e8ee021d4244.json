\n            name:'内容',\r\n            icon:'el-icon-video-play',\r\n            component:rightView,\r\n            children:[\r\n               {\r\n                 path:'post-article',\r\n                 hidden: false,\r\n                 name:'发表文章',\r\n                 icon:'el-icon-edit',\r\n                 component:postArticle\r\n               },\r\n               {\r\n                 path:'manager-article',\r\n                 hidden: false,\r\n                 name:'文章管理',\r\n                 icon:'el-icon-monitor',\r\n                 component:articleManager\r\n               },{\r\n                 path:'manager-comment',\r\n                 hidden: false,\r\n                 name:'评论管理',\r\n                 icon:'el-icon-news',\r\n                 component:managerComment\r\n               },{\r\n                 path:'manager-image',\r\n                 hidden: false,\r\n                 name:'图片管理',\r\n                 icon:'el-icon-bangzhu',\r\n                 component:managerImage\r\n               }\r\n            ]\r\n          },\r\n          {\r\n            path: '/user',\r\n            hidden: false,\r\n            name:'用户',\r\n            icon:'el-icon-user',\r\n            component:rightView,\r\n            children:[\r\n               {\r\n                 path:'list',\r\n                 hidden: false,\r\n                 name:'用户列表',\r\n                 icon:'el-icon-s-data',\r\n                 component:list\r\n               },\r\n               {\r\n                 path:'email',\r\n                 hidden: false,\r\n                 name:'邮箱设置',\r\n                 icon:'el-icon-umbrella',\r\n                 component:email\r\n               },{\r\n                 path:'info',\r\n                 hidden: false,\r\n                 name:'用户信息',\r\n                 icon:'el-icon-box',\r\n                 component:info\r\n               },{\r\n                 path:'rest-password',\r\n                 hidden: false,\r\n                 name:'密码设置',\r\n                 icon:'el-icon-goods',\r\n                 component:restPassword\r\n               }\r\n            ]\r\n          },\r\n          {\r\n            path: '/operation',\r\n            hidden: false,\r\n            name:'运营',\r\n            icon:'el-icon-s-marketing',\r\n            component:rightView,\r\n            children:[\r\n               {\r\n                 path:'loop',\r\n                 hidden: false,\r\n                 name:'轮播图管理',\r\n                 icon:'el-icon-c-scale-to-original',\r\n                 component:loopManager\r\n               },\r\n               {\r\n                 path:'manager-category',\r\n                 hidden: false,\r\n                 name:'分类管理',\r\n                 icon:'el-icon-headset',\r\n                 component:categoryManager\r\n               }\r\n            ]\r\n          },\r\n          {\r\n            path: '/settinss',\r\n            hidden: false,\r\n            name:'设置',\r\n            icon:'el-icon-setting',\r\n            component:rightView,\r\n            children:[\r\n               {\r\n                 path:'friend-link',\r\n                 hidden: false,\r\n                 name:'友情链接',\r\n                 icon:'el-icon-coffee-cup',\r\n                 component:friendLink\r\n               },\r\n               {\r\n                 path:'web-info',\r\n                 hidden: false,\r\n                 name:'网站信息',\r\n                 icon:'el-icon-dessert',\r\n                 component:webInfo\r\n               }\r\n            ]\r\n          }\r\n      ]\r\n    },\r\n    {\r\n      path:'/login',\r\n      component:login\r\n    },\r\n    {\r\n      path: \"/forbidden\",\r\n      component:forbidden\r\n     }\r\n]\r\n\r\nconst originalPush = VueRouter.prototype.push\r\nVueRouter.prototype.push = function push (location, onResolve, onReject) {\r\n  if (onResolve || onReject) return originalPush.call(this, location, onResolve, onReject)\r\n  return originalPush.call(this, location).catch(err => err)\r\n}\r\n\r\n\r\n\r\nconst router = new VueRouter({\r\n  routes // (缩写) 相当于 routes: routes\r\n})\r\n\r\nexport default router\r\n"]}]}